SHELL=/bin/bash
include ../../../include/Makefile_include

OBJS =  AMB2HDF_nk.o  gnrt_at_nk.o getcal_nk.o \
        gnrt_fname_nk.o  bias_corr_nk.o read_biastable_nk.o \
        openfs.o read_parm.o read_systim.o  init.o ramb_whdfeos.o \
        ramb_hd.o ramb_scn.o scn_data.o  cnt2rd.o rad2at.o qc_at.o \
        toolkit_td.o set_sw.o sw_def.o def_dim.o def_geo.o def_data.o \
        gnrt_stime.o jd2md.o leapyear.o sw_wrt.o  wrt_limit.o wrt_scal.o \
        wrt_navig.o wrt_geo.o wrt_data.o read_latbox_table.o read_mask.o \
        mask_stype.o 

INCLUDE =  -I$(HDFEOS_INC) -I$(HDF_INC)
LIBS	=  -L$(HDFEOS_LIB) -L$(HDF_LIB) -L${SZIP_LIB} -lhdfeos -lGctp -lmfhdf -ldf -ljpeg -lz -lm -lsz 


CMD = ${BIN_DIR}/runamsub_pre_nk

all : $(CMD)


$(CMD) : $(OBJS)
	$(CC) $(INCLUDE) -o $(@) $(OBJS) $(LIBS)

.c.o:
	${CC} $(INCLUDE) -c $<

clean:
	-rm -f $(OBJS) $(CMD)
